{
  "name": "fashion-ai-backend",
  "version": "2.0.0",
  "type": "module",
  "description": "Fashion AI Video Generator - Backend API",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "sync:models": "node sync-models.js",
    "seed-options": "node seedOptions.js",
    "cleanup-options": "node cleanup-options.js",
    "list-models": "node list-all-models.js",
    "test": "node test-analysis-models.js",
    "test:gemini": "node test-gemini.js",
    "test:fireworks": "node test-fireworks.js",
    "test:byteplus": "node test-byteplus.js",
    "test:zai": "node test-zai.js",
    "test:nvidia": "node -e \"import('./services/nvidiaService.js').then(m => m.analyzeWithNVIDIA('test-images/anh-nhan-vat.jpeg', 'Describe this image').then(r => console.log(r)).catch(e => console.error(e.message)))\"",
    "test:mistral": "node -e \"import('./services/mistralService.js').then(m => m.analyzeWithMistral('test-images/anh-nhan-vat.jpeg', 'Describe this image').then(r => console.log(r)).catch(e => console.error(e.message)))\"",
    "test:groq": "node -e \"import('./services/groqService.js').then(m => m.analyzeWithGroq('test-images/anh-nhan-vat.jpeg', 'Describe this image').then(r => console.log(r)).catch(e => console.error(e.message)))\"",
    "test:all": "node test-all-providers.js",
    "test:providers": "node test-all-providers.js",
    "test:upload": "node test-image-upload.js",
    "test:upload-ai": "node test-upload-with-ai.js",
    "test:flow": "node test-full-flow.js",
    "test:flow:auto": "node test-full-flow.js --analysis auto --image-gen auto --video-gen auto",
    "test:flow:claude": "node test-full-flow.js --analysis claude",
    "test:flow:gpt4": "node test-full-flow.js --analysis gpt4",
    "test:flow:gemini": "node test-full-flow.js --analysis gemini",
    "test:flow:zai": "node test-full-flow.js --analysis zai",
    "test:flow:nvidia": "node test-full-flow.js --analysis nvidia",
    "test:flow:mistral": "node test-full-flow.js --analysis mistral",
    "test:flow:fast": "node test-full-flow.js --skip-image-gen --skip-video-gen",
    "test:flow:no-cleanup": "node test-full-flow.js --no-cleanup",
    "test:flow:verbose": "node test-full-flow.js --verbose",
    "test:browser": "node test-browser-automation.js",
    "test:browser:zai": "node test-browser-automation.js --service zai",
    "test:browser:zai-login": "node test-browser-automation.js --service zai --wait-login",
    "test:browser:grok": "node test-browser-automation.js --service grok",
    "test:browser:both": "node test-browser-automation.js --service both",
    "test:browser:debug": "node test-browser-automation.js --slow --screenshot --timeout 120",
    "login:zai": "node scripts/zai-login.js",
    "login:grok": "node scripts/grok-login.js",
    "login:grok:auto": "node scripts/grok-auto-login.js",
    "grok:test:full": "node test-grok-full-flow.js",
    "session:list": "node scripts/session-manager.js list",
    "session:info": "node scripts/session-manager.js info",
    "session:delete": "node scripts/session-manager.js delete",
    "session:clear": "node scripts/session-manager.js clear",
    "grok:full": "node test-grok-full-workflow.js",
    "grok:full:video": "node test-grok-full-workflow.js --video",
    "grok:analyze": "node test-grok-analyze.js",
    "grok:generate": "node test-grok-generate.js",
    "test:full": "npm run test:upload && npm run test:upload-ai && npm run test:all",
    "test:multi-flow": "node test-multi-flow.js",
    "test:openrouter": "node test-openrouter.js",
    "test:openrouter:api": "node test-openrouter-api.js",
    "seed:providers": "node scripts/seedImageProviders.js",
    "test:keys": "node scripts/testApiKeys.js",
    "seed:image-providers": "node seedImageProviders.js",
    "seed:templates": "node scripts/seedCorePromptTemplates.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@google/generative-ai": "^0.21.0",
    "@openrouter/sdk": "^0.8.0",
    "axios": "^1.13.5",
    "chalk": "^5.6.2",
    "cloudinary": "^2.9.0",
    "commander": "^14.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.3.1",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.5",
    "googleapis": "^171.4.0",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.23.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "openai": "^4.20.0",
    "ora": "^9.3.0",
    "puppeteer": "^21.11.0",
    "replicate": "^0.25.2",
    "socket.io": "^4.7.0"
  },
  "devDependencies": {
    "bcryptjs": "^3.0.3",
    "canvas": "^3.2.1",
    "nodemon": "^3.0.1",
    "playwright": "^1.58.2",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  }
}
